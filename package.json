{
    "name": "@kele23/appscript-vite-plugin",
    "description": "Appscript vite plugin",
    "version": "1.0.3",
    "author": "Michele Scala (scala233@gmail.com)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kele23/appscript-vite-plugin"
    },
    "license": "MIT",
    "keywords": [
        "vite",
        "vite-plugin",
        "appscript",
        "gas"
    ],
    "type": "module",
    "sideEffects": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src",
        "!src/**/*.test.ts"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup"
    },
    "peerDependencies": {
        "vite": "^6.0.0 || ^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.2.1",
        "oxlint": "^1.11.0",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
